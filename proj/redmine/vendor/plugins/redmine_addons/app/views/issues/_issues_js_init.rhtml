<script type="text/javascript">
  window.IN_PROGRESS_STATUS_ID = '<%= IssueStatus.find_by_id(RedmineAddons::Config.settings["status_ids"]["in_progress"]).position %>';
  window.BACK_URL = '<%= url_for(params) %>';
  attachListEditInitHandler();
function selectAllOptions(id)
{
  var select = $(id);
  for (var i=0; i<select.options.length; i++) {
    select.options[i].selected = true;
  }
}


  function submit_query_form(id) {
    selectAllOptions("selected_columns");
    $(id).submit();
  }
</script>
